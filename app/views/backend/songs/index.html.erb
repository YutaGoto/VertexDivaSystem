<div class="contents ml_10">
  <table class="pure-table pure-table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>曲名</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.id %></td>
          <td><%= song.title %></td>
          <td><%= link_to :show, backend_song_path(song.id), class: 'btn btn-warning btn-xs' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to '新規登録', new_backend_song_path, class: 'btn btn-info' %>

</div>
