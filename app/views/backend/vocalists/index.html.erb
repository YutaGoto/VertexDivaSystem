<div class="contents ml_10">
  <table class="pure-table pure-table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>ボカロ名</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @vocalists.each do |vocalist| %>
        <tr>
          <td><%= vocalist.id %></td>
          <td><%= vocalist.name %></td>
          <td><%= link_to "show", backend_vocalist_path(vocalist.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to "new", new_backend_vocalist_path %>

</div>
